create table baseinfo(
 id             int  primary key  auto_increment,
 hospitalname   varchar(100) character set utf8 default NULL   comment '医院名称',
 hospitaltype   varchar(50)       comment '医院类型',
 directorname   varchar(50)    comment '科主任姓名',
 directorphone  varchar(20)    comment '手机',
 directoremail  varchar(50)    comment '邮箱',
 messagername   varchar(50)     comment '信息员姓名',
 messagerphone  varchar(20)   comment '信息员手机号',
 messageremail  varchar(50)   comment '信息员邮箱',
 ot_1           varchar(50)   comment '住院部手术室使用台数',
 ot_2           varchar(50)  comment '日间(门诊)手术室使用台数',
 ot_3           varchar(50)  comment '介入治疗(需麻醉)台数',
 ot_4           varchar(50)  comment '内镜(无痛)检查台数',
 ot_5  varchar(50)  comment '人流及无痛分娩台数',
 nob_1 varchar(50) comment '麻醉后监护室(PACU)',
 nob_2 varchar(50) comment '麻醉科ICU(AICU)',
 nob_3 varchar(50) comment '麻醉科疼痛病房',
 nob_4 varchar(50) comment '日间手术麻醉后恢复室',
 ana_fixed_doctor varchar(50) comment '麻醉科固定在岗（本院）医师总数',
 ana_total_work varchar(50) comment '同期麻醉科完成麻醉总例次数（万例次）',
 ana_per varchar(50) comment '麻醉科医患比%',

 ana_total_doctor varchar(50) comment '麻醉科医师总数',
 ana_aicu varchar(50) comment 'AICU',
 ana_pain varchar(50) comment '疼痛诊疗',
 ana_other varchar(50) comment '其它',
 ana_total varchar(50) comment '总人数',
 ana_nurse_total varchar(50) comment '麻醉科护士总人数临床麻醉（含PACU）',
 ana_nurse_aicu varchar(50) comment 'aicu',
 ana_nurse_pain varchar(50) comment '疼痛诊疗',
 ana_nurse_other varchar(50) comment '其它',
 nurse_total varchar(50) comment '总人数',
 create_time datetime  comment '创建时间',
 update_time datetime  comment '修改时间'
)ENGINE=InnoDB DEFAULT CHARSET=utf8;


create table qualityinfo(
 id             int  primary key  auto_increment,
 username   varchar(100)  character set utf8 default NULL    comment '填表人姓名',
 unitname   varchar(100)   character set utf8 default NULL      comment '单位名称',
 workload_count   varchar(20)    comment '住院部手术室麻醉总数',
 workload_rsa_count  varchar(20)    comment '麻醉总例次数',
 workload_rsa_1  varchar(20)    comment '1级',
  workload_rsa_2   varchar(20)     comment '2级',
  workload_rsa12_per  varchar(20)   comment '1~2级的比例',
  workload_rsa_3  varchar(20)   comment '3级',
  workload_rsa_4  varchar(20)   comment '4级',
  workload_rsa_5  varchar(20)  comment '5级',
  workload_rsa345_per   varchar(20)  comment '3-5级所占比例',
  workload_emergency_count  varchar(20)  comment '急诊手术-例次数',
  workload_emergency_per  varchar(20)  comment '急诊手术-所占比例',
  workload_ana_count   varchar(20) comment '全身麻醉-总数',
  workload_ana_enter  varchar(20) comment '全身麻醉-吸入',
  workload_ana_static varchar(20) comment '全身麻醉-静脉',
  workload_ana_complex varchar(20) comment '全身麻醉-复合',
  workload_can_count   varchar(20) comment '椎管麻醉数-总数',
  workload_can_waist  varchar(20) comment '椎管麻醉数-腰麻',
  workload_can_out varchar(20) comment '椎管麻醉数-硬膜外',
  workload_can_unit varchar(20) comment '椎管麻醉数-腰硬联合',
  workload_nerve_count varchar(20) comment '神经干-总数',
  workload_nerve varchar(20) comment '神经干',
  workload_nerve_plexus varchar(20) comment '神经从',
  workload_other_count varchar(20) comment '总数',
  workload_other_mac varchar(20) comment 'mac',
  workload_other_other varchar(20) comment '其它',
  workload_specialty_brain varchar(20) comment '脑外科麻醉',
  workload_specialty_chest varchar(20) comment '胸外科麻醉',
  workload_specialty_cardiovascular varchar(20) comment '心血管外科麻醉',
  workload_specialty_obstetrics varchar(20) comment '产科麻醉',
  workload_specialty_child varchar(20) comment '小儿科麻醉',
  workload_specialty_old varchar(20) comment '老年科麻醉',
  workload_specialty_nopain varchar(20) comment '无痛内窥镜深度镇静、麻醉数',
  workload_specialty_day varchar(20) comment '日间手术室麻醉数',
  workload_painless_count varchar(20) comment '无痛分娩及无痛人流数',
  workload_invasive_tract varchar(20) comment '胃肠道',
  workload_invasive_breathing varchar(20) comment '呼吸系统',
  workload_invasive_health varchar(20) comment '心血管',
  workload_invasive_operation varchar(20) comment '介入手术',
  workload_invasive_stream varchar(20) comment '无痛人流',
  workload_invasive_childbirth varchar(20) comment '分娩',
  workload_invasive_other varchar(20) comment '其它',
  live_pacu varchar(20) comment '收住或诊治病人数',
  live_aicu varchar(20) comment 'aicu',
  live_pain varchar(20) comment '疼痛病房',
  live_nerve_trunk_count varchar(20) comment '同期总数',
  live_nerve_trunk_success varchar(20) comment '成功例数',
  live_nerve_trunk_per varchar(20) comment '成功率',
  live_block_count varchar(20) comment '硬膜外阻滞成功率-同期总数',
  live_block_sucess varchar(20) comment '硬膜外阻滞成功率-成功例数',
  live_block_per varchar(20) comment '硬膜外阻滞成功率-成功率',
  live_difficulty_count varchar(20) comment '困难气道处理成功率-同期总数',
  live_difficulty_sucess varchar(20) comment '困难气道处理成功率-成功例数',
  live_difficulty_per varchar(20) comment '困难气道处理成功率-成功率',
  live_ultrasonic_count varchar(20) comment '超声引导传刺-同期总数',
  live_ultrasonic_count2 varchar(20) comment '超声引导传刺-应用超声引导例数',
  live_ultrasonic_per varchar(20) comment '超声引导传刺-成功率',
  live_anaesthesia_cancel_count varchar(20) comment '麻醉开始后手术取消例数',
  live_anaesthesia_count varchar(20) comment '麻醉开始后手术取消例数-同期总数',
  live_anaesthesia_cancel_per varchar(20) comment '麻醉开始后手术取消例数-取消率',
  live_noplain_count varchar(20) comment '非计划二次气管插管率-例数',
